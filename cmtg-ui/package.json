{
  "name": "cmtg-ui",
  "version": "0.0.107",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "buildProd": "ng build --configuration production --output-hashing=all",
    "test": "ng test",
    "lint": "eslint -c .eslintrc.json -f junit src > eslint_out.xml",
    "runLint": "ng lint --format junit >./eslint_out.xml",
    "buildwar": "npm run buildProd && npm run pack",
    "pack": "cd dist/cmtg-ui/ && echo 'Package cmtg-ui.war' && jar cvf ../cmtg-ui.war *",
    "installCommonLib": "npm --userconfig ./npmrc install --no-save rbn-common-lib@14.1.x --legacy-peer-deps"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@ng-idle/core": "^13.0.0",
    "@ngx-formly/core": "6.0.0",
    "@ngx-formly/primeng": "6.0.0",
    "@ngx-formly/schematics": "6.0.0",
    "@ngx-translate/core": "^13.0.0",
    "calculate-size": "^1.1.1",
    "font-awesome": "^4.7.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.23",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.45",
    "ngx-translate-multi-http-loader": "^3.0.0",
    "pako": "^2.1.0",
    "primeflex": "2.0.0",
    "primeicons": "5.0.0",
    "primeng": "14.2.2",
    "rxjs": "^6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "14.0.3",
    "@angular-eslint/eslint-plugin-template": "14.0.3",
    "@angular-eslint/schematics": "14.0.3",
    "@angular-eslint/template-parser": "14.0.3",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "^8.18.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "typescript": "~4.7.2"
  }
}
